// version.rc.in
#include <version.h>



1 VERSIONINFO
FILEVERSION PROJECT_VERSION_SYSTEM_FILE
 PRODUCTVERSION PROJECT_VERSION_SYSTEM_PRODUCT
 FILEFLAGSMASK 0x17L
#ifdef _DEBUG
 FILEFLAGS 0x1L
#else
 FILEFLAGS 0x0L
#endif
 FILEOS 0x4L
 FILETYPE 0x0L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
        VALUE "CompanyName", PROJECT_OWNER_COMPANY_NAME
        VALUE "FileDescription", "library to converter aa/aax file"
        VALUE "FileVersion", PROJECT_VERSION_DISPLAY_FILE
        VALUE "InternalName", "${LIBAUDIBLECONVERTER}.dll"
        VALUE "LegalCopyright", PROJECT_OWNER_LEGAL_COPYRIGHT
        VALUE "OriginalFilename", "${LIBAUDIBLECONVERTER}.dll"
        VALUE "ProductName", PROJECT_OWNER_PRODUCT_NAME
        VALUE "ProductVersion", PROJECT_VERSION_DISPLAY_PRODUCT
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END